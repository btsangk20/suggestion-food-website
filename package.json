{
  "name": "admin-codebase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn lint-fix && next dev",
    "build": "yarn lint-fix && next build",
    "start": "yarn lint-fix && next start",
    "lint": "next lint",
    "lint-fix": "next lint --dir . --fix && yarn format",
